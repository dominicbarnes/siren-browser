{
  "entries": [
    "index.html",
    "app/**/*.js",
    "lib/**/*.js"
  ],
  "plugins": [
    [ "mako-stat", [ "html", "js", "css" ] ],
    [ "mako-buffer", [ "html", "js", "css" ] ],
    [ "mako-html" ],
    [ "mako-css" ],
    [ "mako-babel", { "sourceMaps": true } ],
    [ "mako-output", [ "html", "js", "css" ] ],
    [ "mako-copy", [ "html" ] ],
    [ "mako-write", [ "js", "css" ] ]
  ]
}
